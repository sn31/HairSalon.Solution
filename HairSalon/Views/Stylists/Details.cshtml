@{
  Layout = "_Layout";
}
<div class="jumbotron">
    <h1>Stylists</h1>
</div>
<div class="container">
  <table class="table">
  <thead>
    <tr>
      <th scope="col">Client Name</th>
    </tr>
  </thead>
  @foreach (var client in @Model["client"])
  {
  <tbody>
    <tr>
      <td>@client.Name</td>
      
    </tr>
    
  </tbody>

  }
  </table>

</div>